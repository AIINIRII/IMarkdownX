<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xyz.aiinirii.imarkdownx">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:name=".IMarkdownXApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.user.changepassword.ChangePasswordActivity" />
        <activity android:name=".ui.user.updateinfo.UpdateInfoActivity" />
        <activity android:name=".ui.user.register.RegisterActivity" />
        <activity android:name=".ui.user.login.LoginActivity" />
        <activity android:name=".ui.changeprivatepassword.ChangePrivatePasswordActivity" />
        <activity android:name=".ui.edit.privacy.PrivacyActivity" />
        <activity android:name=".ui.edit.render.EditRenderActivity" />
        <activity android:name=".ui.edit.main.EditMainActivity" />
        <activity android:name=".MainActivity"/>
    </application>

</manifest>